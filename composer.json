{
    "name": "teepluss/hmvc",
    "description": "Laravel HMVC.",
    "keywords": ["laravel", "hmvc"],
    "homepage": "https://github.com/teepluss/laravel-hmvc",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Teepluss",
            "email": "teepluss@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "5.3.*@dev"
    },
    "require-dev": {
        "illuminate/support": "4.1.x"
    },
    "autoload": {
        "classmap": [
            "src/Teepluss/Hmvc/Exceptions.php"
        ],
        "psr-0": {
            "Teepluss\\Hmvc": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev"
}
